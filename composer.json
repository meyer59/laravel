{
    "name": "meyer59/scaffolding",
    "description": "Reliese Components for Laravel Framework code generation.",
    "keywords": ["reliese", "laravel"],
    "homepage": "http://cristianllanos.com",
    "support": {
        "issues": "https://github.com/reliese/laravel/issues",
        "source": "https://github.com/reliese/laravel"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Llanos",
            "email": "cristianllanos@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "~2.5",
         "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0",
        "illuminate/database": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0",
        "illuminate/contracts": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0",
        "illuminate/filesystem": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0",
        "illuminate/console": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Reliese\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reliese\\Coders\\CodersServiceProvider"
            ]
        }
    }
}
